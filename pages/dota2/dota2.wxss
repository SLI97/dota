page {
	height: 100%;
}

.container {
	min-height: 100%;
	background-image: url(data:image/jpg;base64,);
	background-size: 100% auto;
	background-repeat: repeat-y;
	background-position: center top;
	padding: 70rpx 0 0 0;
}

.logo-wrap{
	position: fixed;
	padding:10rpx 30rpx;
	background-color: #0D1012;
	top: 0;
	width:100%;
	z-index: 999;
	display: flex;
	justify-content: space-between;
	align-items: center;
	box-sizing: border-box;
}

.btn{
	padding:18rpx 30rpx;
	line-height: 26rpx;
	font-size: 26rpx;
	text-align: center;
	background-image: linear-gradient(to right, rgb(166,54,37) , rgb(198,96,43));
	color:#fff;

}

.logo {
	display: block;
	width: 346rpx;
	height: 50rpx;
}

.dota-row {
	position: relative;
	margin-top: 120rpx;
	min-height: 600rpx;
}

.dota-row .main{
	min-height: 600rpx;
}

.dota-row image {
	width: 100%;
	transition: all 1s ease;
}

.fadeIn {
	animation: fade-in 1s ease-in-out 1;
}

.dota-row image.kaer-ball{
	position: absolute;
	top:0;
	left:0;
}

.dota-row .float,.dota-col .float  {
	animation: float 6s ease-in-out infinite;
}

.wrap{
	transition: all 1s ease;
}

.dota-col{
	position: absolute;
	width: 30%;
	transition: all 1s ease;
	top:0%;
}

.dota-col image{
	position: relative;
}

.dota-col.left{
	left:0;
}

.dota-col.right{
	right:0;
}

.dota-col .reward-name{
	font-size: 24rpx;
	padding: 4rpx 30rpx;
}

.dota-col .reward-label{
	font-size: 18rpx;
	padding: 4rpx 10rpx;
}

.reward-container{
	position: absolute;
	bottom:30rpx;
	display: flex;
	flex-direction: column;
	justify-content: center;
	align-items: center;
	text-align: center;
	width:100%;
}

.reward-name {
	font-size: 32rpx;
	letter-spacing: 4rpx;
	font-weight: bold;
	padding: 4rpx 30rpx;
	text-transform: uppercase;
	color: #BAE816;
	font-family: "Reaver", serif;
	filter: drop-shadow(4rpx 4rpx 6rpx #000000) drop-shadow(0rpx 0rpx 20rpx #53A146);
}

.reward-label {
	padding: 10rpx 30rpx;
	font-size: 24rpx;
	letter-spacing: 4rpx;
	text-transform: uppercase;
	color: #ffffff90;
	filter: drop-shadow(4rpx 4rpx 6rpx #000000) drop-shadow(0rpx 0rpx 20rpx #53A146);
}

@keyframes float {
	0% {
		top: 0rpx;
	}

	50% {
		top: 50rpx;
	}

	100% {
		top: 0rpx;
	}
}

@keyframes fade-in {
	0% {
		opacity: 0;
		transform: translateX(0%) translateY(100rpx);
	}

	100% {
		opacity: 1;
		transform: translateX(0%) translateY(0);
	}
}